{
  "schemaVersion": 1,
  "id": "adventure_map_utilities",
  "version": "1.0.6",

  "name": "Adventure Map Utilities",
  "description": "This mod helps the client to interact with an adventure map, even if the mod is not present on the server!",
  "authors": [
    "Caoten_hoch2"
  ],
  "contact": {
    "homepage": "https://caotenhoch2.wixsite.com/caotenhoch2",
    "sources": "https://github.com/Caotenhoch2/AdventureMapUtilities"
  },

  "license": "MIT",
  "icon": "assets/adventure_map_utilities/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "eu.caoten.adventure_map_utilities.Main"
    ],
    "client": [
      "eu.caoten.adventure_map_utilities.Client"
    ],
    "modmenu": [
      "eu.caoten.adventure_map_utilities.config.ModMenu"
    ]
  },
  "mixins": [
    "adventure_map_utilities.mixins.json"
